{
    "name": "route-planner",
    "version": "1.0.0",
    "description": "Web app to plan routes with large numbers of waypoints",
    "repository": "https://github.com/rizadh/route-planner",
    "scripts": {
        "lint": "npm run lint:tsc && npm run lint:tslint",
        "lint:tsc": "tsc -p . -noEmit",
        "lint:tslint": "tslint --project . --format verbose -c tslint.json",
        "clean": "rm -rf public",
        "build": "npm run clean && npm run build:parcel",
        "build:parcel": "parcel build --out-dir public --no-source-maps src/index.html",
        "serve": "npm run clean && npm run serve:parcel",
        "serve:parcel": "parcel --out-dir public --https src/index.html",
        "test": "jest"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@hot-loader/react-dom": "^16.11.0",
        "@reduxjs/toolkit": "^1.2.1",
        "@types/apple-mapkit-js": "^5.18.0",
        "@types/file-saver": "^2.0.1",
        "@types/lodash": "^4.14.149",
        "@types/react": "^16.9.17",
        "@types/react-beautiful-dnd": "^11.0.4",
        "@types/react-dom": "^16.8.4",
        "@types/react-redux": "^7.1.5",
        "@types/react-textarea-autosize": "^4.3.4",
        "@types/uuid": "^3.4.4",
        "babel-polyfill": "^6.26.0",
        "copy-text-to-clipboard": "^2.1.0",
        "file-saver": "^2.0.2",
        "immer": "^5.2.1",
        "ismobilejs": "^1.0.3",
        "lodash": "^4.17.15",
        "query-string": "^6.7.0",
        "rc-progress": "^2.3.0",
        "react": "^16.8.6",
        "react-beautiful-dnd": "^12.2.0",
        "react-dom": "^16.12.0",
        "react-hot-loader": "^4.12.18",
        "react-redux": "^7.1.3",
        "react-textarea-autosize": "^7.1.0",
        "redux": "^4.0.5",
        "redux-observable": "^1.1.0",
        "rxjs": "^6.5.4",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/jest": "^24.0.25",
        "@types/redux-logger": "^3.0.7",
        "autoprefixer": "^9.6.0",
        "jest": "^24.9.0",
        "parcel": "^1.12.4",
        "prettier": "^1.18.2",
        "prettier-tslint": "^0.4.2",
        "redux-logger": "^3.0.6",
        "sass": "^1.24.4",
        "ts-jest": "^24.3.0",
        "tslint": "^5.17.0",
        "tslint-react": "^4.0.0",
        "tslint-react-hooks": "^2.1.1",
        "typescript": "^3.7.4"
    },
    "alias": {
        "react-dom": "@hot-loader/react-dom"
    }
}
