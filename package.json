{
    "name": "route-planner",
    "version": "2021.06.20",
    "author": "Rizadh Nizam",
    "description": "Web app to plan routes with large numbers of waypoints",
    "license": "MIT",
    "repository": "https://github.com/rizadh/route-planner",
    "scripts": {
        "lint": "prettier -c . && eslint src --max-warnings 0 --cache",
        "lint:fix": "prettier -w . && eslint src --cache --fix",
        "clean": "rm -rf public",
        "build": "parcel build --out-dir public --no-source-maps src/index.html",
        "build:tsc": "tsc -noEmit -p .",
        "develop": "parcel --out-dir out --https src/index.html",
        "test": "jest",
        "generate": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@apollo/react-hooks": "^4.0.0",
        "@hot-loader/react-dom": "^17.0.0-rc.2",
        "@reduxjs/toolkit": "^1.4.0",
        "@types/apple-mapkit-js": "^5.50.0",
        "@types/file-saver": "^2.0.1",
        "@types/haversine": "^1.1.4",
        "@types/lodash": "^4.14.162",
        "@types/react": "^16.9.52",
        "@types/react-beautiful-dnd": "^13.0.0",
        "@types/react-dom": "^16.8.4",
        "@types/react-redux": "^7.1.5",
        "@types/react-transition-group": "^4.2.3",
        "@types/resize-observer-browser": "^0.1.4",
        "@types/styled-components": "^5.1.4",
        "@types/uuid": "^8.3.0",
        "apollo-boost": "^0.4.7",
        "b64-to-blob": "^1.2.19",
        "babel-polyfill": "^6.26.0",
        "copy-text-to-clipboard": "^2.1.0",
        "file-saver": "^2.0.2",
        "graphql": "^15.3.0",
        "haversine": "^1.1.1",
        "immer": "^7.0.9",
        "ismobilejs": "^1.0.3",
        "lodash": "^4.17.20",
        "pdf-lib": "^1.11.2",
        "query-string": "^6.13.5",
        "react": "^16.8.6",
        "react-beautiful-dnd": "^13.0.0",
        "react-dom": "^16.12.0",
        "react-hot-loader": "^4.13.0",
        "react-is": "^16.12.0",
        "react-redux": "^7.2.1",
        "react-spring": "^8.0.27",
        "react-textarea-autosize": "^8.2.0",
        "react-transition-group": "^4.3.0",
        "redux": "^4.0.5",
        "redux-observable": "^1.1.0",
        "rxjs": "^6.6.3",
        "styled-components": "^5.2.0",
        "uuid": "^8.3.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^1.17.10",
        "@graphql-codegen/typescript": "1.17.10",
        "@graphql-codegen/typescript-operations": "1.17.8",
        "@types/jest": "^26.0.14",
        "@types/redux-logger": "^3.0.7",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "autoprefixer": "^9.8.6",
        "concurrently": "^5.3.0",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.4",
        "eslint-plugin-react-hooks": "^4.1.2",
        "husky": "^4.3.0",
        "jest": "^26.5.3",
        "parcel": "^1.12.4",
        "prettier": "^2.1.2",
        "redux-logger": "^3.0.6",
        "sass": "^1.27.0",
        "ts-jest": "^26.4.1",
        "typescript": "^4.0.3"
    },
    "alias": {
        "react-dom": "@hot-loader/react-dom"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn lint"
        }
    },
    "jest": {
        "preset": "ts-jest"
    }
