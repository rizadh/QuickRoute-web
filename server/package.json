{
    "name": "quickroute",
    "version": "1.0.0",
    "description": "Web app to plan routes with large numbers of waypoints",
    "repository": "https://github.com/rizadh/route-planner",
    "scripts": {
        "clean": "rm -rf out",
        "lint": "tsc -p . -noEmit && tslint --project . --format verbose -c tslint.json",
        "lint:watch": "onchange src/**/*.ts src/*.ts -- tslint --project . --format verbose -c tslint.json",
        "build": "tsc -p .",
        "build:watch": "tsc -w -p .",
        "start": "node out/server.js",
        "start:watch": "dotenv nodemon out/server.js"
    },
