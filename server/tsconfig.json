{
    "includes": "src",
    "compilerOptions": {
        "outDir": "out",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "module": "commonjs",
        "target": "es2015",
        "strict": true,
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowSyntheticDefaultImports": true
    }
}
